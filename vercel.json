{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dashboard/build",
  "installCommand": "npm install && cd dashboard && npm install",
  "framework": null,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 